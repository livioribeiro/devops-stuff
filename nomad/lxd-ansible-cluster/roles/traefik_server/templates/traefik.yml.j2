entryPoints:
  web:
    # Listen on port 8081 for incoming requests
    address: ":80"

providers:
  file:
    filename: /etc/traefik/config.yml
  consulCatalog:
    exposedByDefault: false
    defaultRule: "Host(`{% raw %}{{ .Name }}{% endraw %}.service.{{ ansible_eth0.ipv4.address }}.nip.io`)"

api:
  dashboard: true
  insecure: true